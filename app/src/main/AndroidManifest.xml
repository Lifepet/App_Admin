<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.app_admin">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
            android:allowBackup="true"
            android:icon="@drawable/ic_main"
            android:label="@string/app_name"
            android:networkSecurityConfig="@xml/network_security_config"
            android:roundIcon="@drawable/ic_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activity.VolunteerWriteActivity">
        </activity>
        <activity android:name=".activity.ProtectWriteActivity">
        </activity>
        <activity android:name=".activity.AdoptWriteActivity">
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".SignUpActivity">
        </activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.DetailAdoptActivity"/>
        <activity android:name=".activity.DetailProtectActivity"/>
        <activity android:name=".activity.DetailVolunteerActivity"/>
    </application>

</manifest>